<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="health.my.santosh.healthinsurance">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher22"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher22"
        android:supportsRtl="true"
        android:theme="@style/slashtheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/slashtheme" />
        <activity
            android:name=".Main2Activity"
            android:screenOrientation="portrait"
            android:theme="@style/slashtheme" />
        <activity
            android:name=".HospitalDetail"
            android:screenOrientation="portrait"
            android:theme="@style/slashtheme" />
        <activity
            android:name=".Slash"
            android:screenOrientation="portrait"
            android:theme="@style/slashtheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Searchdata"
            android:screenOrientation="portrait"
            android:theme="@style/slashtheme" />
        <activity android:name=".Download"></activity>
    </application>

</manifest>